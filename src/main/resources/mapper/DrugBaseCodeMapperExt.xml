<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qybx.mapper.DrugBaseCodeMapperExt" >

    
  
  <select id="selectAll" resultType = "com.qybx.po.BaseIndex">
  
    SELECT
    drug_base_code AS baseCode,
    drug_base_name AS baseName
    FROM drug_base_code
    WHERE enable = 1
  
  </select>
</mapper>